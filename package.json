    "dashboard:correlation": "tsx src/tools/generateDashboardCorrelation.ts auditoria.csv ./reports/charts ./reports/risk",
    "dashboard:risk:embed": "tsx src/tools/generateDashboardRiskEmbed.ts auditoria.csv ./reports/charts ./reports/risk",
    "dashboard:risk": "tsx src/tools/generateDashboardRisk.ts auditoria.csv ./reports/charts ./reports/risk",
