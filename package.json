{
  "name": "backtester-pro",
  "version": "1.0.0",
  "type": "module",
  "description": "Supermotor de backtesting con IA, TypeScript y Node ESM",
  "main": "dist/server_ai.js",
"scripts": {
  "render:test": "tsc -p _buildtest/tsconfig.buildtest.json && node dist_buildtest/server_test.js",
  "build:test": "tsc -p _buildtest/tsconfig.buildtest.json",
"start:test": "node dist_buildtest/server_test.js",
  "dev": "tsx src/server_unified.ts",
"build": "tsc -p tsconfig.render.json",
"start": "node dist/server_unified.js",
  "render": "tsc -p tsconfig.render.json && node dist/server_unified.js",
  "ai:server": "npm run build && node dist/server_ai.js",
  "reflex:v15plus": "tsx src/ai/reflex_v15_plus.ts",
  "reflex:v15": "tsx src/ai/reflex_v15.ts",
  "dashboard:pdf": "tsx src/tools/exportCognitiveReportPDF.ts",
  "dashboard:risk": "tsx src/tools/generateDashboardRisk.ts auditoria.csv ./reports/charts ./reports/risk",
  "dashboard:live": "tsx src/tools/generateDashboardLive.ts",
  "dashboard:control": "tsx src/tools/generateControlCenter.ts",
  "dashboard:correlation": "tsx src/tools/generateDashboardCorrelation.ts auditoria.csv ./reports/charts ./reports/risk",
  "dashboard:risk:embed": "tsx src/tools/generateDashboardRiskEmbed.ts auditoria.csv ./reports/charts ./reports/risk",
  "risk:analyze": "tsx src/tools/riskAnalyzer.ts auditoria.csv ./reports/risk",
  "audit:dashboard:legal": "tsx src/tools/generateDashboard.ts auditoria.csv ./reports/charts",
  "audit:dashboard": "tsx src/tools/generateDashboard.ts auditoria.csv ./reports/charts",
  "opt": "tsx src/runOptimize.ts",
  "robust": "tsx src/runRobust.ts",
  "ai-opt": "tsx src/runAiOptimizer.ts",
  "ai-adaptive": "tsx src/runAiAdaptive.ts",
  "gen:synthetic": "tsx src/tools/genSynthetic.ts",
  "gen:trend": "tsx src/tools/genSyntheticTrend.ts",
  "compare": "tsx src/runCompare.ts",
  "monte": "tsx src/runMonte.ts",
  "monte:plus": "tsx src/runMontePlus.ts",
  "test:decimal": "tsx runTestDecimal.ts",
  "audit": "tsx verifyAudit.ts",
  "manifests:list": "tsx src/tools/listManifests.ts",
  "audit:charts": "tsx src/tools/generateCharts.ts auditoria.csv ./reports/charts",
  "postinstall": "echo 'Skip Prisma generate on Render'"
},

  "keywords": [
    "backtesting",
    "ai",
    "typescript",
    "finanzas",
    "trading",
    "machine-learning",
    "quant",
    "strategy",
    "optimization",
    "automated-trading"
  ],
  "author": "Julio Cesar",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^5.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.18.13",
    "@types/pg": "^8.15.5",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/seedrandom": "^3.0.8",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@types/node": "^20.11.30",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.4.1",
    "body-parser": "^2.2.0",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.6.0",
    "decimal.js-light": "^2.5.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "pdfmake": "^0.1.72",
    "pdfmake-wrapper": "^2.1.3",
    "pg": "^8.16.3",
    "plotly.js-dist-min": "^3.1.2",
    "prisma": "^5.18.0",
    "seedrandom": "^3.0.5",
    "typescript": "^5.4.5",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  }
}
